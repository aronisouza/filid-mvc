RewriteEngine On
Options All -Indexes

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-l

RewriteRule ^(.*) /index.php?pag=$1

ErrorDocument 403 https://github.com/aronisouza

<Files .env>
    Order allow,deny
    Deny from all
</Files>

<Files autoload.php>
    Order allow,deny
    Deny from all
</Files>
